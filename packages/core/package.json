{
  "name": "interweave",
  "version": "13.1.0",
  "description": "React library to safely render HTML, filter attributes, autowrap text, autolink, and much more.",
  "keywords": [
    "interweave",
    "inject",
    "interpolate",
    "react",
    "string",
    "component",
    "element",
    "matcher",
    "filter",
    "clean",
    "email",
    "url",
    "autolink",
    "emoji",
    "emojibase",
    "hashtag",
    "html",
    "markup",
    "safe",
    "render",
    "html"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.js"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/interweave.git",
    "directory": "packages/core"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "dependencies": {
    "escape-html": "^1.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "interweave-ssr": "^2.0.0",
    "react": "^17.0.2"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.tsx"
    },
    "platform": "browser"
  }
}
