  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/interweave.git",
    "directory": "packages/ssr"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "dependencies": {
    "parse5": "^6.0.1",
    "style-parser": "^1.1.1"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  }
}
