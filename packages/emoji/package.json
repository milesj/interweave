{
  "name": "interweave-emoji",
  "version": "6.0.1",
  "description": "Emoji support for Interweave.",
  "keywords": [
    "interweave",
    "react",
    "emoji",
    "emojibase"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.js"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/interweave.git",
    "directory": "packages/emoji"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "dependencies": {
    "emojibase": "^5.2.0",
    "emojibase-regex": "^5.1.3"
  },
  "peerDependencies": {
    "interweave": "^12.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "emojibase-test-utils": "^6.2.2",
    "react": "^17.0.2"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "platform": "browser"
  }
}
